   "source": [
    "### Number of bits set ####\n",
    "\n",
    "def countNoOfbitsSet(num):\n",
    "    count = 0\n",
    "    while(num):\n",
    "        if(num & 1 == 1):\n",
    "            count = count+1\n",
    "        num >>= 1\n",
    "    return count\n",
    "\n",
    "num=32\n",
    "print(countNoOfbitsSet(num))\n"
   ]
